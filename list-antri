    <script>
      function proses() {
        Swal.fire({
          title: "Are You Sure?",
          text: "Service will be update?",
          icon: "warning",
          showCancelButton: true,
          confirmButtonColor: "#3085d6",
          cancelButtonColor: "#d33",
          confirmButtonText: "Yes!",
        }).then((result) => {
          if (result.isConfirmed) {
            Swal.fire("Data!", "Succes Update.", "success");
          }
        });
      }
